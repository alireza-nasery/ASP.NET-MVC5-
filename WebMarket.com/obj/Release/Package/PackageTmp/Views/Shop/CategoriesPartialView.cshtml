@model IEnumerable<DataLayer.Group>
<div class="accordion-group" id="tourStep2">
    <div class="accordion-heading">
        <a class="accordion-toggle" data-toggle="collapse" href="#Grouping">دسته بندی<b class="caret"></b></a>
    </div>
    <div id="Grouping" class="accordion-body collapse in">
        <div class="accordion-inner">
            @foreach (var item in Model)
            {
                <a href="#" data-target=".Categories_@item.GroupID" class="selectable"><i class="box"></i>@item.GroupName</a>
            }
        </div>
    </div>
</div>
